<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springlec.base.dao.HospitalDao">
	<select id="hospitalDao" resultType="com.springlec.base.model.HospitalDto">
		SELECT * FROM hospital1
	</select>
	
	<select id="hospitalLocationDao" resultType="com.springlec.base.model.HospitalDto">
		SELECT hpid ,dutyName ,wgs84Lat ,wgs84Lon 
		FROM hospital1
		WHERE dutyName LIKE #{dutyName};
	</select>
</mapper>